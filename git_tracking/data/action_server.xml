<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_check_changes" model="ir.cron">
            <field name="name">Check Changes</field>
            <field name="model_id" ref="model_module_data"/>
            <field name="state">code</field>
            <field name="code">env['module.data'].sudo().update_cron_data()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>